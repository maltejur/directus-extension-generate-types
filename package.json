{
  "name": "directus-extension-generate-types",
  "version": "0.4.2",
  "license": "MPL-2.0",
  "author": {
    "name": "Malte JÃ¼rgens",
    "email": "maltejur@dismail.de",
    "url": "https://github.com/maltejur"
  },
  "contributors": [
    {
      "name": "Jean Claveau",
      "email": "jean.claveau@gmail.com",
      "url": "https://github.com/jclaveau"
    }
